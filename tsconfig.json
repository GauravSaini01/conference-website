{
  "compilerOptions": {
    "jsx": "preserve",
    "strict": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noEmit": true,
    "incremental": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "cypress/e2e/Footer.cy.js",
    "cypress/e2e/Landing.cy.js",
    "cypress/e2e/Navbar.cy.js",
    "cypress/e2e/Venue.cy.js",
    "cypress/support/commands.js",
    "cypress/support/e2e.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
